# .env.example
# Example configuration for the AI Command Helper.
# Copy this file to .env and fill in your specific values.
# The .env file itself should NOT be committed to Git.

# The full URL for the Ollama API generate endpoint
OLLAMA_API_URL=http://localhost:11434/api/generate

# The default Ollama model to use if not specified via command line
OLLAMA_DEFAULT_MODEL=gemma3:1b